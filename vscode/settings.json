{
    "workbench.activityBar.visible": false,
    "window.menuBarVisibility": "toggle",
    "editor.fontSize": 15,
    "editor.fontFamily": "'Source Code Pro Medium'",
    "editor.minimap.renderCharacters": false,
    "editor.rulers": [
        80
    ],
    "editor.minimap.enabled": false,
    "vim.useSystemClipboard": true,
    "go.lintTool": "gometalinter",
    "go.lintFlags": [
        "--fast",
        "--aggregate",
        "--disable=gotype"
    ],
    "go.buildFlags": ["-tags=integration"],
    "go.useLanguageServer": false,
    "problems.decorations.enabled": false,
    "editor.lineNumbers": "on",
    "update.channel": "none",
    "window.titleBarStyle": "native",
    "workbench.editor.showTabs": false,
    "explorer.openEditors.visible": 0,
    "workbench.sideBar.location": "left",
    "go.enableCodeLens": {
        "references": false,
        "runtest": false
    },
    "workbench.colorCustomizations": {
        "sideBar.background": "#21252E",
        "statusBar.background": "#21252E",
        "editorCursor.foreground": "#61AFEF",
        "widget.shadow": "#ffffff00",
        "scrollbar.shadow": "#ffffff00",
        "gitDecoration.modifiedResourceForeground": "#E5C07B",
        "gitDecoration.deletedResourceForeground": "#E06C75",
        "gitDecoration.untrackedResourceForeground": "#98C379",
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "variable.other.property.ts",
                "settings": {
                    "foreground": "#abb2bf",
                }
            },
            {
                "scope": "variable.other.object.ts",
                "settings": {
                    "foreground": "#abb2bf",
                }
            },
            {
                "scope": "variable.other.object.property.ts",
                "settings": {
                    "foreground": "#abb2bf",
                }
            }
        ]
    },
    "window.zoomLevel": 0,
    "go.formatTool": "goimports",
    "editor.overviewRulerBorder": false,
    "breadcrumbs.enabled": true,
    "workbench.colorTheme": "One Dark Pro",
    "vim.hlsearch": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "gitlens.keymap": "alternate",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "editor.renderWhitespace": "all"
}
